
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Welcome to Book store Application</h1>
    @for (int i = 0; i < 5; i++)
    {
        <h1>Welcome to Book store Application</h1>
    }
    @{ 
        var list = new List<int>() { 1, 2, 3, 4, 5 };
    }
    @foreach(var item in list)
    {
        <h5>@item</h5>
    }
</body>
</html>
